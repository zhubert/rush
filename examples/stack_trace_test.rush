# Test stack trace functionality

# Function that throws an error
throwError = fn() {
    throw RuntimeError("Something went wrong in throwError")
}

# Function that calls throwError
middleFunction = fn() {
    throwError()
}

# Function that calls middleFunction
topFunction = fn() {
    middleFunction()
}

# Test try-catch with stack trace
try {
    topFunction()
} catch (error) {
    print("Error:", error.message)
    print("Stack trace:")
    print(error.stack)
}

print("Stack trace test completed")