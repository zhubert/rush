# Comprehensive error handling test

print("=== Error Handling Test ===")

# Test 1: Basic try-catch with property access
print("\nTest 1: Basic error properties")
try {
    throw RuntimeError("Something went wrong")
} catch (error) {
    print("Caught:", error.type, "-", error.message)
}

# Test 2: Type-specific catch with properties  
print("\nTest 2: Type-specific error handling")
try {
    throw ValidationError("Input validation failed")
} catch (ValidationError error) {
    print("Validation error:", error.message)
} catch (error) {
    print("Other error:", error.message)
}

# Test 3: Try-catch-finally with properties
print("\nTest 3: Try-catch-finally")
try {
    throw TypeError("Wrong type provided")
} catch (error) {
    print("Error details:")
    print("  Type:", error.type)
    print("  Message:", error.message)
} finally {
    print("Cleanup completed")
}

print("\n=== All tests completed ===")