text = "hello world"
print("Original text:", text)
print("Text length:", text.length)

upper_text = text.upper()
print("Upper case:", upper_text)

lower_text = upper_text.lower()
print("Lower case:", lower_text)

arr = [1, 2, 3]
print("Original array:", arr)
print("Array length:", arr.length)

arr.push(4)
print("After push:", arr)

popped = arr.pop()
print("Popped value:", popped)
print("After pop:", arr)

joined = arr.join(" | ")
print("Joined:", joined)